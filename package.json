{
  "name": "rebanna",
  "version": "0.5.8",
  "description": "Rebanna is build to easily create a webfont with support of stacked icons.",
  "main": "./dist/cli.js",
  "scripts": {
    "babel": "npx babel src --watch --out-dir dist",
    "build": "node ./dist/cli build",
    "build:watch": "node ./dist/cli build --watch",
    "clean": "node ./dist/cli clean",
    "compress": "node ./dist/cli compress",
    "split": "node ./dist/cli split",
    "test": "node ./node_modules/eslint/bin/eslint.js \"src/**\""
  },
  "repository": "github:maartenbrakkee/rebanna",
  "keywords": [
    "svg",
    "icons",
    "stacked",
    "icon font",
    "font"
  ],
  "author": "Maarten Brakee <maarten@zeroten.nl>",
  "license": "MIT",
  "homepage": "https://github.com/maartenbrakkee/rebanna",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "eslint": "^6.8.0"
  },
  "dependencies": {
    "async": "3.2.0",
    "chalk": "3.0.0",
    "chokidar": "3.3.0",
    "meow": "6.1.0",
    "nunjucks": "3.2.1",
    "rimraf": "3.0.2",
    "svgo": "1.3.2",
    "webfont": "9.0.0",
    "xml2js": "0.4.23"
  },
  "bin": "./dist/cli.js"
}
